{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: cl.exe 활성 파일 빌드",
            "command": "cl.exe",
            "args": [
                "/Zi",
                "/EHsc",
                "/nologo",
                "/Fe${fileDirname}\\bin\\debug\\${fileBasenameNoExtension}.exe",
                "/Fd:${workspaceFolder}\\obj\\debug\\vc140.pdb",
                "/Fo:${workspaceFolder}\\obj\\debug\\${fileBasenameNoExtension}.obj",
                "${file}"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$msCompile"
            ],
            "group": "build",
            "detail": "디버거에서 생성된 작업입니다."
        }
    ],
    "version": "2.0.0"
}